{
	"name": "vue-styleguidist",
	"description": "Vue components style guide generator",
	"version": "2.0.0",
	"homepage": "https://github.com/viljamis/vue-styleguidist",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/viljamis/vue-styleguidist.git"
	},
	"bugs": {
		"url": "https://github.com/viljamis/vue-styleguidist/issues"
	},
	"license": "MIT",
	"main": "scripts/index.js",
	"bin": {
		"vue-styleguidist": "bin/styleguidist.js"
	},
	"files": ["bin", "lib", "loaders", "scripts"],
	"engines": {
		"node": ">=4",
		"npm": ">=3"
	},
