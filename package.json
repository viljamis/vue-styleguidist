		"react-codemirror2": "^3.0.7",
		"react-dev-utils": "^4.2.1",
		"react-dom": "^16.2.0",
		"react-group": "^1.0.5",
		"react-icons": "^2.2.7",
		"remark": "^8.0.0",
		"semver-utils": "^1.1.1",
		"strip-comments": "^0.4.4",
		"style-loader": "^0.19.1",
		"to-ast": "^1.0.0",
		"type-detect": "^4.0.5",
		"uglifyjs-webpack-plugin": "1.1.5",
		"unist-util-visit": "^1.3.0",
		"vue": "^2.4.2",
		"vue-docgen-api": "^2.3.4",
		"vue-loader": "^13.3.0",
